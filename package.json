{
  "name": "rest-api-to-graphql",
  "version": "1.0.0",
  "description": "Obtener resultados de un Api Rest y Pasarlo a GraphQL",
  "main": "index.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon \"src/server.ts\" --exec \"ts-node\" src/server.ts -e ts,graphql"
  },
  "author": "enrique villasana <enrique.villasana26@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-datasource-rest": "^0.8.1",
    "apollo-server": "^2.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.0.0",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.14",
    "graphql-tools": "^5.0.0",
    "http": "0.0.1-security",
    "moment": "^2.24.0",
    "ncp": "^2.0.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^13.13.0"
  }
}
